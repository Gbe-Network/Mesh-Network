generator client { provider = "prisma-client-js" }
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Subscription {
  id         String  @id @default(cuid())
  user       String
  startSlot  BigInt
  expirySlot BigInt
  createdAt  DateTime @default(now())
}

model SwapEvent {
  id        String   @id @default(cuid())
  action    String
  amount    Float
  slot      BigInt
  createdAt DateTime @default(now())
}
